package milestone1.lecture7.assignment;

import java.util.Scanner;

public class ReverseNumber {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int N = s.nextInt();
		int rem = 0;
		while(N>0) {
			rem = N%10;
			if(rem != 0) {
				System.out.print(rem);
			}
			N /= 10;
		}

	}

}
